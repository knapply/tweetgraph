citHeader("To cite tweetgraph use:")

citEntry(
  entry        = "Manual",
  title        = "tweetgraph: Tweet Graph Structures for Network Analysis",
  author       = as.person("Brendan Knapp"),
  year         = "2019",
  note         = "R package version 0.0.0.9",
  url          = "https://knapply.github.io/tweetgraph",
  key          = "tweetgraph-package",
  textVersion  = paste(
    "Knapp, B. G. (2019). tweetgraph: Tweet Graph Structures for Network Analysis",
    "Data. R package version 0.0.0.9",
    "Retrieved from https://knapply.github.io/tweetgraph"
  )
)